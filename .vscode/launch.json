{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Limpia Imagenes",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "console": "integratedTerminal"
    },
    {
      "name": "Limpia Imagenes (C++ GUI)",
      "type": "node-terminal",
      "request": "launch",
      "command": "cmake --build cpp_app/build && ./cpp_app/build/limpia_imagenes_gui",
      "cwd": "${workspaceFolder}"
    }
  ]
}
